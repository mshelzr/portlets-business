<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.ext.portlet.pin.model.impl.CategoriaModelImpl">
		<attributes>
			<id name="categoriaId">
				<column name="CODIGO_CATEGORIA" />
			</id>
			<basic name="idImagen">
				<column name="ID_IMAGE" />
			</basic>
			<basic name="nombre">
				<column name="NOMBRE" />
			</basic>
			<basic name="estado">
				<column name="ESTADO" />
			</basic>
			<basic name="descripcion">
				<column name="DESCRIPCION" />
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.ext.portlet.pin.model.impl.PersonaModelImpl">
		<attributes>
			<id name="userId">
				<column name="USERID" />
			</id>
			<basic name="nombres">
				<column name="NOMBRES" />
			</basic>
			<basic name="apellidoPaterno">
				<column name="APELLIDO_PATERNO" />
			</basic>
			<basic name="apellidoMaterno">
				<column name="APELLIDO_MATERNO" />
			</basic>
			<basic name="dni">
				<column name="DNI" />
			</basic>
			<basic name="tipoDocumento">
				<column name="TIPO_DOCUMENTO" />
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.ext.portlet.pin.model.impl.CategoriaImpl" name="Categoria">
		<table name="Categoria" />
		<attributes />
	</entity>
	<entity class="com.ext.portlet.pin.model.impl.PersonaImpl" name="Persona">
		<table name="Persona" />
		<attributes />
	</entity>
</entity-mappings>